<thead>
    <tr>
        <th style="background-color: #b8b6b6; cursor: pointer">First Name</th>
        <th style="background-color: #b8b6b6; cursor: pointer">Last Name</th>
        <th class="center aligned" style="background-color: #b8b6b6;"> </th>
    </tr>
</thead>
<tbody class="fixed sticky">
    {{#each dentist}}
        <tr class="top aligned">
            <td>{{firstname}}</td>
            <td>{{lastname}}</td>
            <td class="center aligned">
                <button class="ui button" data-id="{{_id}}">Edit</button>
            </td>
        </tr>
    {{/each}}
</tbody>