{{!-- dentist tab --}}
<table class="ui striped table" style="table-layout: fixed;">
<thead>
    <tr>
    <th style="background-color: #b8b6b6;">First Name</th>
    <th style="background-color: #b8b6b6;">Last Name</th>
    <th class="center aligned" style="background-color: #b8b6b6;"> </th>
    </tr>
</thead>
<tbody class="fixed sticky">
    {{#each dentist}}
    <tr>
    <td>{{firstname}}</td>
    <td>{{lastname}}</td>
    <td class="center aligned">
        <button class="ui button" data-id="{{_id}}">Edit</button>
    </td>
    </tr>
    {{/each}}
    <tr>
        <div class="ui icon button" data-tooltip="Add New Dentist" data-position="bottom center" id="add-dentist-button" style="padding-top: 15px;">
            <i class="large user plus icon" id="add-dentist-button"></i>
        </div>
    </tr>
</tbody>
</table>