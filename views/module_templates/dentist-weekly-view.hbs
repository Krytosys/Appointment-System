{{#each data}}
    <tr class="row-date">
        <td colspan="2" class="center aligned">
            <span>{{dayCaps}}</span>
            {{#ifEquals dayCaps "SUN"}}
                <a class="anchor" id="one"></a>
            {{/ifEquals}}
            {{#ifEquals dayCaps "MON"}}
                <a class="anchor" id="two"></a>
            {{/ifEquals}}
            {{#ifEquals dayCaps "TUE"}}
                <a class="anchor" id="three"></a>
            {{/ifEquals}}
            {{#ifEquals dayCaps "WED"}}
                <a class="anchor" id="four"></a>
            {{/ifEquals}}
            {{#ifEquals dayCaps "THU"}}
                <a class="anchor" id="five"></a>
            {{/ifEquals}}
            {{#ifEquals dayCaps "FRI"}}
                <a class="anchor" id="six"></a>
            {{/ifEquals}}
            {{#ifEquals dayCaps "SAT"}}
                <a class="anchor" id="seven"></a>
            {{/ifEquals}}
        </td>
        <td colspan="12" class="borderless-column">
            <span>{{dateShort}}</span>
        </td>
    </tr>
    {{#each appointment}}
        <tr class="appointment-row" data-id="{{_id}}">
            <td colspan="2" class="center aligned details-row" data-id="{{_id}}">
                {{time}}<br>
                {{#getEndTime time date}}
                {{/getEndTime}}
            </td>
            <td colspan="12" data-id="{{_id}}">
                <span class="ui large text">
                    <b>{{firstname}} {{lastname}}</b>
                </span>
                <div class="ui medium text" style="color:#b5b5b5" data-id="{{_id}}">
                    {{patientcontact}} <br>
                    {{#each process}}
                        {{commafy @index}}{{processname}}
                    {{/each}}
                </div>
            </td>
        </tr>
    </td>
    {{/each}}
{{/each}}
