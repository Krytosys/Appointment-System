{{!-- procedure tab --}}
<table class="ui striped table" style="table-layout: fixed;">
<thead>
    <tr>
    <th style="background-color: #b8b6b6;">Name</th>
    <th class="center aligned" style="background-color: #b8b6b6;"> </th>
    </tr>
</thead>
<tbody class="fixed sticky">
    {{#each procedure}}
    <tr>
    <td>{{processname}}</td>
    <td class="center aligned">
        <button class="ui button" data-id="{{_id}}">Edit</button>
        <button class="negative ui button" data-id="{{_id}}" data-name="{{processname}}">Delete</button>
    </td>
    </tr>
    {{/each}}
    <tr>
        <div class="ui icon button" data-tooltip="Add New Procedure" data-position="bottom center" id="add-procedure-button" style="padding-top: 15px;">
            <i class="large plus square outline icon" id="add-procedure-button"></i>
        </div>
    </tr>
</tbody>
</table>